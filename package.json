{
  "name": "time-crunch-list",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=8.2.1"
  },
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 ./dist/server.js",
    "dev": "npm run watch",
    "build": "npm run clean:dist && concurrently \"npm run build:server\" \"npm run build:client\" \"npm run build:scss\"",
    "build:server": "NODE_ENV=production webpack --colors --display-error-details --config configs/webpack.server.js",
    "build:client": "NODE_ENV=production webpack --colors --display-error-details --config configs/webpack.client.js",
    "build:scss": "gulp scss",
    "clean:dist": "del 'dist/**'",
    "lint": "gulp lint",
    "lint:scss": "gulp scsslint",
    "lint:js": "gulp eslint",
    "lint:package": "gulp packagelint",
    "lint:json": "gulp jsonlint",
    "test": "npm run lint && npm run build:scss && npm run test:unit && npm run test:browser && npm run test:coverage",
    "test:browser": "NODE_ENV=test gulp browser-tests",
    "test:coverage": "gulp summate-coverage-reports",
    "test:unit": "NODE_ENV=test nyc mocha --check-leaks --globals 'window,document,history,navigator' --recursive --require babel-register $(find tests -path '*_spec.js')",
    "watch": "npm run clean:dist && concurrently --kill-others \"npm run watch:server-start\" \"npm run watch:server\" \"npm run watch:client\" \"npm run watch:scss\"",
    "watch:client": "NODE_ENV=development webpack-dev-server --config configs/webpack.client-watch.js",
    "watch:scss": "gulp watch",
    "watch:server": "NODE_ENV=development webpack --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch:server-start": "node node_modules/just-wait -p \"dist/**\" && nodemon ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benhunsaker/time-crunch-list.git"
  },
  "author": "Ben Hunsaker <ben.hunsaker@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benhunsaker/time-crunch-list/issues"
  },
  "homepage": "https://github.com/benhunsaker/time-crunch-list#readme",
  "dependencies": {
    "boom": "^4.3.1",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.1.1",
    "hapi-mongodb": "^6.2.1",
    "inert": "^4.2.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "concurrently": "^3.5.0",
    "del": "^3.0.0",
    "del-cli": "^1.1.0",
    "eslint-config-hapi": "^10.0.0",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-react": "^7.1.0",
    "forever": "^0.15.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-eslint": "^4.0.0",
    "gulp-jsonlint": "^1.2.0",
    "gulp-nice-package": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-task-loader": "^1.4.4",
    "gulp-util": "^3.0.8",
    "json-loader": "^0.5.4",
    "just-wait": "^1.0.11",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.4",
    "map-stream": "0.0.7",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.1.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-node-externals": "^1.6.0"
  }
}
